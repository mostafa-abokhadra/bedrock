
{
  "version": 2,
  "builds": [
    {
      "src": "dist/server.js",
      "use": "@vercel/node",
      "config": { 
        "maxLambdaSize": "50mb"
      }
    }
  ],
  
  "routes": [
    {
      "src": "/api-docs/(.*)",
      "dest": "dist/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "dist/server.js"
    }
  ]
  
}